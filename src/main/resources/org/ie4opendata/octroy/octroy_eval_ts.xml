    </typeDescription>
    <typeDescription>
      <name>org.apache.uima.ruta.type.EvalAnnotation</name>
      <description/>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>original</name>
          <description/>
          <rangeTypeName>uima.tcas.Annotation</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>org.apache.uima.ruta.type.FalseNegative</name>
      <description/>
      <supertypeName>org.apache.uima.ruta.type.EvalAnnotation</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>org.apache.uima.ruta.type.TruePositive</name>
      <description/>
      <supertypeName>org.apache.uima.ruta.type.EvalAnnotation</supertypeName>
    </typeDescription>
  </types>
